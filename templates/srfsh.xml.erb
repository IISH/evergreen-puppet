<?xml version="1.0"?>
<!-- This file follows the standard bootstrap config file layout found in opensrf_core.xml -->
<srfsh>
  <router_name>router</router_name>
  <domain><%= private_hosts %></domain>
  <username><%= opensrf_user %></username>
  <passwd><%= opensrf_password %></passwd>
  <port>5222</port>
  <logfile><%= sysdir %>/var/log/srfsh.log</logfile>
  <loglevel>4</loglevel>
  <client>true</client>
</srfsh>
